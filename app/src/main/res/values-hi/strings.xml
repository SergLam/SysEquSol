    <!-- Main Activity -->
    <string name="app_name">समीकरणों की व्यवस्था\nका समाधान</string>
    <string name="main_text">सिस्टम का आकार चुनें:\n(समीकरणों में चर की संख्या)</string>
    <string name="main_faq_vis">सैद्धांतिक सामग्री दिखाएं\n(क्रेमर की विधि)</string>
    <string name="main_faq_hide">सैद्धांतिक सामग्री छुपाएं\n(क्रेमर की विधि)</string>

    <!-- Theoretical strings -->
    <string name="theory1">क्रैमर का नियम अज्ञात के लिए समान समीकरणों के साथ रैखिक समीकरणों की एक प्रणाली को सुलझाने के लिए एक स्पष्ट फार्मूला है, हर बार जब सिस्टम में एक अनूठा समाधान होता है तो मान्य है। यह समीकरणों के दाहिने हाथों के वेक्टर द्वारा एक कॉलम की जगह, गुणांक के मैट्रिक्स के निर्धारकों (वर्ग) के संदर्भ में समाधान को अभिव्यक्त करता है और इसके द्वारा प्राप्त मैट्रिक्स को दर्शाता है।\n\nसमीकरणों की एक प्रणाली को हल करने का उदाहरण:</string>
    <string name="step1">पहले आपको सिस्टम के मुख्य निर्धारक की गणना करने की आवश्यकता है:</string>

    <string name="src_det_lb">det=</string>
    <string name="src_det">"  5  4 \n  3  2 "</string>
    <string name="src_det_solution">"= 5*2 - 3*4 = -2"</string>

    <string name="theory2">यदि प्रमुख निर्धारक शून्य है, तो सिस्टम में असीम रूप से कई समाधान होते हैं या समाधान नहीं होते हैं इस मामले में गॉस पद्धति का उपयोग करना आवश्यक है।\n\nयदि प्रमुख निर्धारक शून्य नहीं है, तो सिस्टम में केवल एक ही समाधान है। समीकरणों की जड़ों को खोजने के लिए, दो और निर्धारकों की गणना करना आवश्यक है:</string>

    <string name="det1_lb">"det1="</string>
    <string name="det1">"  9  4 \n  7  2 "</string>
    <string name="det1_sol">="9*2 - 7*4 = -10"</string>

    <string name="det2_lb">"det2="</string>
    <string name="det2">"  5  9 \n  3  7 "</string>
    <string name="det2_sol">"=5*7 - 9*3 = 8"</string>

    <string name="theory3">समीकरण की जड़ें सूत्रों द्वारा पाई जा सकती हैं:</string>

    <!-- Calculation of result-->
    <string name="var1">"Y = "</string>
    <string name="var1_calc_up">det2</string>
    <string name="var1_calc_down">det</string>
    <string name="equal">" = "</string>
    <string name="var1_calc_up_val">8</string>
    <string name="var1_calc_down_val">-2</string>
    <string name="var1_val">" = -4"</string>

    <string name="var2">"X = "</string>
    <string name="var2_calc_up">det1</string>
    <string name="var2_calc_down">det</string>
    <string name="var2_calc_up_val">-10</string>
    <string name="var2_calc_down_val">-2</string>
    <string name="var2_val">" = 5"</string>

    <string name="theory_end">बड़ी संख्या में चर के साथ समीकरणों की प्रणालियों को उसी सिद्धांत से हल किया जाता है</string>

    <!-- Main Activity end-->

