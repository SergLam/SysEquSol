    <!-- Main Activity -->
    <string name="app_name">Решение систем\nуравнений</string>
    <string name="main_text">Выберите размер системы:\n(Кол-во переменных в уравнениях)</string>
    <string name="main_faq_vis">Показать теоретический материал\n(метод Крамера)</string>
    <string name="main_faq_hide">Скрыть теоретический материал\n(метод Крамера)</string>

    <!-- Theoretical strings -->
    <string name="theory1">Правило Крамера представляет собой явную формулу для решения системы линейных уравнений с таким же числом уравнений, как и для неизвестных, справедливым всякий раз, когда система имеет единственное решение. Оно выражает решение в терминах определителей (квадратной) матрицы коэффициентов и матриц, полученных из нее, заменяя один столбец на вектор правых частей уравнений.\n\nПример решения системы уравнений:</string>
    <string name="step1"> Сначала необходимо вычислить главный определитель системы:</string>

    <string name="src_det_lb">det=</string>
    <string name="src_det">"  5  4 \n  3  2 "</string>
    <string name="src_det_solution">"= 5*2 - 3*4 = -2"</string>

    <string name="theory2">Если главный определитель, равен нулю, то система имеет бесконечно много решений или вообще не имеет решений. В этом случае необходимо использовать метод Гаусса.\n\nЕсли главный определитель не равен нулю - система имеет только одно решение. Чтобы найти корни уравнений, необходимо вычислить еще два определителя:</string>

    <string name="det1_lb">"det1="</string>
    <string name="det1">"  9  4 \n  7  2 "</string>
    <string name="det1_sol">="9*2 - 7*4 = -10"</string>

    <string name="det2_lb">"det2="</string>
    <string name="det2">"  5  9 \n  3  7 "</string>
    <string name="det2_sol">"=5*7 - 9*3 = 8"</string>

    <string name="theory3">"Корни уравнения можно найти по формулам:"</string>

    <!-- Calculation of result-->
    <string name="var1">"Y = "</string>
    <string name="var1_calc_up">det2</string>
    <string name="var1_calc_down">det</string>
    <string name="equal">" = "</string>
    <string name="var1_calc_up_val">8</string>
    <string name="var1_calc_down_val">-2</string>
    <string name="var1_val">" = -4"</string>

    <string name="var2">"X = "</string>
    <string name="var2_calc_up">det1</string>
    <string name="var2_calc_down">det</string>
    <string name="var2_calc_up_val">-10</string>
    <string name="var2_calc_down_val">-2</string>
    <string name="var2_val">" = 5"</string>

    <string name="theory_end">Системы уравнений с большим числом переменных решаются по тому же принципу</string>

